//  Zones
UpdatePlayerZone(playerid)
{
	SetPVarInt(playerid, "LastSAZone", _GetPlayerZone(playerid));
}

stock GetPlayerZone(playerid)
{
	return GetPVarInt(playerid, "LastSAZone");
}

stock GetZoneSound(zone)
{
	if(0 <= zone < sizeof(Zones))	
	{
		return Zones[zone][zone_sound];
	}
	return 0;
}

stock GetPlayerArea(playerid)
{
	new string[32] = "No-one";
	new zone = GetPlayerZone(playerid);
	if(0 <= zone < sizeof(Zones))
	{
		strput(string, Zones[zone][zone_name]);
	}
	return string;
}

stock _GetPlayerZone(playerid)
{
	new interior = GetPlayerInterior(playerid);
	for(new i = 0; i < sizeof Zones; i++)
	{
	    if(IsPlayerInSquare(playerid, Zones[i][zone_minx], Zones[i][zone_miny], Zones[i][zone_maxx], Zones[i][zone_maxy], false) && interior == Zones[i][zone_interior])
	    {
			return i;
	    }
	}
	return INVALID_ZONE;
}

stock GetPointZone(Float:x, Float:y, interior = 0)
{
	for(new i = 0; i < sizeof(Zones); i++)
	{
		if(x > Zones[i][zone_minx] && y > Zones[i][zone_miny] && x < Zones[i][zone_maxx] && y < Zones[i][zone_maxy] && interior == Zones[i][zone_interior])
		{
			return i;
		}
	}
	return INVALID_ZONE;
}

stock GetPointArea(Float:x, Float:y)
{
	new string[32] = "No-one";
	new zone = GetPointZone(x, y);
	if(0 <= zone < sizeof(Zones))
	{
		strput(string, Zones[zone][zone_name]);
	}
	return string;
}